{
  "name": "homemaker",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "dependencies": {
    "babel": "^5.1.9",
    "bluebird": "^2.3.6",
    "body-parser": "~1.8.1",
    "bookshelf": "^0.7.7",
    "bootstrap": "^3.3.2",
    "compression": "^1.2.2",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "director": "^1.2.5",
    "dotenv": "^0.4.0",
    "es5-shim": "^4.0.5",
    "express": "~4.9.0",
    "hjs": "~0.0.6",
    "http-transport": "^1.1.3",
    "js-beautify": "^1.5.4",
    "jsdom": "^1.3.1",
    "knex": "^0.7.3",
    "less-middleware": "1.0.x",
    "localforage": "^1.2.1",
    "lodash": "^2.4.1",
    "morgan": "~1.3.0",
    "mysql": "^2.5.2",
    "offline-js": "^0.7.11",
    "react": "^0.12.1",
    "react-bootstrap": "^0.13.0",
    "request-promise": "^0.3.2",
    "socket.io": "^1.2.1",
    "socket.io-client": "^1.2.1",
    "uglify-js": "^2.4.20",
    "underscore": "^1.7.0"
  },
  "devDependencies": {
    "babelify": "^6.0.2",
    "browserify": "^8.0.0",
    "envify": "^3.2.0",
    "express-debug": "^1.1.1",
    "watchify": "^2.1.1",
    "uglifyify": "^3.0.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "stage": "0"
        }
      ],
      "envify"
    ]
  },
  "scripts": {
    "watch": "watchify -o public/script.js -v -d ./app/script.js",
    "browser": "browserify ./app/script.js > public/script.js",
    "build": "browserify ./app/script.js | uglifyjs -cm > public/script.js",
    "dev": "npm run watch & npm run server",
    "symlink": "./bin/symlink",
    "server": "node --nouse_idle_notification --expose_gc ./bin/start",
    "inspect": "node-debug -p 8585 --web-host 0.0.0.0 ./bin/start",
    "start": "NODE_ENV=production npm run build & npm run server"
  }
}
