{
  "name": "homemaker",
  "version": "0.0.1",
  "private": true,
  "main": "server.js",
  "dependencies": {
    "bluebird": "^2.3.6",
    "body-parser": "~1.8.1",
    "bookshelf": "^0.7.7",
    "browserify": "^7.0.0",
    "console-polyfill": "^0.1.2",
    "cookie-parser": "~1.3.3",
    "debug": "~2.0.0",
    "dotenv": "^0.4.0",
    "envify": "^3.2.0",
    "es5-shim": "^4.0.5",
    "express": "~4.9.0",
    "hjs": "~0.0.6",
    "js-beautify": "^1.5.4",
    "jsdom": "^1.3.1",
    "knex": "^0.7.3",
    "less-middleware": "1.0.x",
    "morgan": "~1.3.0",
    "mysql": "^2.5.2",
    "node-jsx": "^0.12.4",
    "promise": "^6.0.1",
    "react": "^0.12.1",
    "reactify": "^0.17.1",
    "reflux": "^0.2.1",
    "request-promise": "^0.3.2",
    "superagent-promise": "^0.2.0",
    "uglify-js": "^2.4.15",
    "underscore": "^1.7.0",
    "watchify": "^2.1.1"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "es6": true
        }
      ],
      "envify"
    ]
  },
  "scripts": {
    "watch": "watchify -o public/scripts/client.js -v -d ./client.js",
    "build": "browserify ./client.js | uglifyjs -cm > public/scripts/client.js",
    "dev": "npm run watch & npm run server",
    "symlink": "./bin/symlink",
    "server": "node ./bin/www",
    "start": "npm run server"
  }
}
